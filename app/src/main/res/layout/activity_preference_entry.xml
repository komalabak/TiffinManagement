<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_preference_entry"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.pandadesktop4.loginscreen.PreferenceEntry">



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dates"
        android:id="@+id/text1"
        android:layout_marginLeft="8dp"
        android:textSize="20dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text3"
        android:layout_below="@+id/text1"
        android:text="1"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="20dp"
        android:textSize="20dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text4"
        android:layout_below="@+id/text3"
        android:layout_marginLeft="25dp"
        android:text="2"
        android:layout_marginTop="8dp"
        android:textSize="20dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text5"
        android:layout_below="@+id/text4"
        android:layout_marginLeft="25dp"
        android:text="3"
        android:textSize="20dp"
        android:layout_marginTop="8dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text6"
        android:layout_below="@+id/text5"
        android:layout_marginLeft="25dp"
        android:text="4"
        android:textSize="20dp"
        android:layout_marginTop="8dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5"
        android:id="@+id/text7"
        android:textSize="20dp"
        android:layout_below="@+id/text6"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="8dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6"
        android:textSize="20dp"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="25dp"
        android:layout_below="@id/text7"
        android:id="@+id/text8"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Make Entry"
        android:textSize="20dp"
        android:id="@+id/text2"
        android:layout_marginLeft="160dp"
        android:layout_marginRight="71dp"
        android:layout_marginEnd="71dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7"
        android:textSize="20dp"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="25dp"
        android:layout_below="@id/text8"
        android:id="@+id/text9"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="8"
        android:textSize="20dp"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="25dp"
        android:layout_below="@id/text9"
        android:id="@+id/text10"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="9"
        android:textSize="20dp"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="25dp"
        android:layout_below="@id/text10"
        android:id="@+id/text11"

        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="10"
        android:textSize="20dp"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@id/text11"
        android:id="@+id/text12"

        />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/text4"
        android:layout_marginLeft="190dp"
        android:id="@+id/checkBox1" />


    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/text5"
        android:layout_marginLeft="190dp"
        android:id="@+id/checkBox2"
        android:layout_marginTop="8dp"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/text6"
        android:layout_marginLeft="190dp"
        android:id="@+id/checkBox3"
        android:layout_marginTop="8dp"/>


    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/text7"
        android:layout_marginLeft="190dp"
        android:id="@+id/checkBox4"
        android:layout_marginTop="8dp"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/text7"
        android:layout_marginLeft="190dp"
        android:id="@+id/checkBox5"
        android:layout_marginTop="8dp"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/text8"
        android:layout_marginLeft="190dp"
        android:id="@+id/checkBox6"
        android:layout_marginTop="8dp"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/text9"
        android:layout_marginLeft="190dp"
        android:id="@+id/checkBox7"
        android:layout_marginTop="8dp"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/text10"
        android:layout_marginLeft="190dp"
        android:id="@+id/checkBox8"
        android:layout_marginTop="8dp"/>


    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/text11"
        android:layout_marginLeft="190dp"
        android:id="@+id/checkBox9"
        android:layout_marginTop="8dp"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/text12"
        android:layout_marginLeft="190dp"
        android:id="@+id/checkBox10"
        android:layout_marginTop="8dp"/>


    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/checkBox10"
        android:layout_marginLeft="190dp"
        android:layout_marginTop="8dp"/>



    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        android:textAllCaps="false"
        android:textColor="#ffff25e6"
        android:layout_below="@+id/text12"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="15dp"/>

</RelativeLayout>
